{
  "name": "@videre/database",
  "version": "1.0.0",
  "main": "build",
  "module": "src",
  "scripts": {
    "start": "docker-compose -f ./bin/docker/docker-compose.yml up -d",
    "prisma-pull": "yarn prisma db pull",
    "prisma-generate": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' yarn prisma generate",
    "build": "rimraf build && swc src -d build",
    "build-watch": "swc src -d build -w"
  },
  "devDependencies": {
    "prisma": "3.6.0"
  },
  "dependencies": {
    "@prisma/client": "3.6.0",
    "@videre/cli": "workspace:^",
    "dotenv": "^9.0.2",
    "postgres": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/videre-project/videre-project.git",
    "directory": "packages/database"
  },
  "prisma": {
    "schema": "bin/prisma/schema.prisma"
  }
}

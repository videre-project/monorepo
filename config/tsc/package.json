{
  "name": "config-tsc",
  "version": "1.0.0",
  "scripts": {
    "tsc": "(cd \"$0\" && yarn run -T tsc $1)",
    "build": "run tsc \"$0\" \"$1\"",
    "watch": "run tsc \"$0\" \"-w $1\"",
    "clean": "run tsc \"$0\" \"-b --clean $1\" && node scripts/clean.cjs \"$0\"",
    "prepack": "run build \"$0\" \"$1\" && run yarn:gh-resolve \"$0\""
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/videre-project/videre-project.git",
    "directory": "config/tsc"
  }
}

{
  "name": "config-jest",
  "version": "1.0.0",
  "scripts": {
    "jest": "(cd \"$0\" && yarn run -T jest $1)",
    "watch": "run jest \"$0\" \"--watch $1\"",
    "clean": "run jest \"$0\" \"--clearCache $1\""
  },
  "dependencies": {
    "@videre/js": "workspace:^",
    "@videre/nodejs": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/videre-project/videre-project.git",
    "directory": "config/jest"
  }
}
